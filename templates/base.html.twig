<!DOCTYPE html>

<html>
    <head>
        <meta charset="UTF-8">

        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="icon" type="image/png" href="{{ asset('build/logo.png') }}" />
            <link rel="stylesheet" href="{{ asset('build/app.css') }}">
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <header>{% include '_navbar.html.twig' %}</header>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        <script src="{{ asset('build/app.js') }}"></script>
        {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
